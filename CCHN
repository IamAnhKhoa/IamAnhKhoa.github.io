    // ============================================================
    
    let allData = [];
    let filteredData = [];
    let selectedRows = new Set();
    let currentPage = 1;
    const rowsPerPage = 50;

    // Kh·ªüi t·∫°o khi m·ªü trang
    window.onload = function() {
        if (RAW_DATA.length > 0) {
            // Th√™m ID cho m·ªói d√≤ng ƒë·ªÉ qu·∫£n l√Ω vi·ªác ch·ªçn
            allData = RAW_DATA.map((row, index) => ({ ...row, _id: index }));
            filteredData = allData;
            
            // L·∫•y ti√™u ƒë·ªÅ t·ª´ d√≤ng ƒë·∫ßu ti√™n
            const headers = Object.keys(RAW_DATA[0]);
            renderTableHeader(headers);
            renderTableBody();
            
            document.getElementById('dataCount').innerText = `T·ªïng: ${allData.length} nh√¢n s·ª±`;
        } else {
            document.getElementById('dataCount').innerText = "Ch∆∞a c√≥ d·ªØ li·ªáu!";
            alert("B·∫°n ch∆∞a d√°n d·ªØ li·ªáu v√†o code! H√£y l√†m theo h∆∞·ªõng d·∫´n.");
        }
    };

    // --- HI·ªÇN TH·ªä B·∫¢NG ---
    function renderTableHeader(headers) {
        const thead = document.querySelector('#dataTable thead');
        thead.innerHTML = '';
        const tr = document.createElement('tr');
        
        // Checkbox All
        const thSelect = document.createElement('th');
        thSelect.className = 'select-col';
        thSelect.innerHTML = '<input type="checkbox" id="selectAll" onclick="toggleSelectAll(this)">';
        tr.appendChild(thSelect);

        headers.forEach(header => {
            if (header !== '_id') {
                const th = document.createElement('th');
                th.innerText = header.replace(/_/g, ' ');
                tr.appendChild(th);
            }
        });
        thead.appendChild(tr);
    }

    function renderTableBody() {
        const tbody = document.querySelector('#dataTable tbody');
        tbody.innerHTML = '';

        const start = (currentPage - 1) * rowsPerPage;
        const end = start + rowsPerPage;
        const pageData = filteredData.slice(start, end);

        if (pageData.length === 0) {
            tbody.innerHTML = '<tr><td colspan="100%" style="text-align:center">Kh√¥ng t√¨m th·∫•y k·∫øt qu·∫£</td></tr>';
            return;
        }

        pageData.forEach(row => {
            const tr = document.createElement('tr');
            
            // Checkbox Row
            const tdSelect = document.createElement('td');
            tdSelect.className = 'select-col';
            const isChecked = selectedRows.has(row._id) ? 'checked' : '';
            tdSelect.innerHTML = `<input type="checkbox" class="row-checkbox" data-id="${row._id}" onclick="toggleRow(${row._id})" ${isChecked}>`;
            tr.appendChild(tdSelect);

            // Data Cells
            Object.keys(row).forEach(key => {
                if (key !== '_id') {
                    const td = document.createElement('td');
                    td.innerText = row[key] || "";
                    tr.appendChild(td);
                }
            });
            tbody.appendChild(tr);
        });
        updatePaginationControls();
    }

    // --- T√åM KI·∫æM ---
    document.getElementById('searchInput').addEventListener('keyup', function(e) {
        const keyword = e.target.value.toLowerCase();
        filteredData = allData.filter(row => {
            return Object.values(row).some(val => String(val).toLowerCase().includes(keyword));
        });
        currentPage = 1;
        renderTableBody();
    });

    // --- CHECKBOX LOGIC ---
    function toggleRow(id) {
        selectedRows.has(id) ? selectedRows.delete(id) : selectedRows.add(id);
    }

    function toggleSelectAll(source) {
        const checkboxes = document.querySelectorAll('.row-checkbox');
        checkboxes.forEach(cb => {
            cb.checked = source.checked;
            const id = parseInt(cb.getAttribute('data-id'));
            source.checked ? selectedRows.add(id) : selectedRows.delete(id);
        });
    }

    // --- XU·∫§T FILE ---
    function formatDateToInt(dateStr) {
        if (!dateStr) return "";
        const parts = dateStr.split('/');
        return (parts.length === 3) ? `${parts[2]}${parts[1]}${parts[0]}` : dateStr;
    }

    function exportSelectedData() {
        if (selectedRows.size === 0) {
            alert("Vui l√≤ng t√≠ch ch·ªçn nh√¢n s·ª± c·∫ßn xu·∫•t!");
            return;
        }

        const maCSKCB = document.getElementById('maCSKCB').value;
        const tuNgay = document.getElementById('ngayBatDau').value;
        const selectedData = allData.filter(item => selectedRows.has(item._id));

        // Mapping d·ªØ li·ªáu IMPORT
        const importData = selectedData.map((item, index) => ({
            "STT": index + 1,
            "MA_CSKCB": maCSKCB,
            "HO_TEN": item.Ho_Ten || "",
            "MACCHN": item.So_CCHN || "",
            "TEN_PHAMVI_CM": item.Pham_Vi_Hanh_Nghe || "",
            "MA_PHAMVI_CM": "",
            "CHUCDANH_NN": "",
            "TEN_VI_TRI": "",
            "MA_VI_TRI": "",
            "DVKT_KHAC": "",
            "THOIGIAN_LV": item.Thoi_Gian_Lam_Viec || "",
            "THOIGIAN_DK": 1,
            "THOIGIAN_NGAY": "",
            "THOIGIAN_TUAN": "",
            "TU_NGAY": tuNgay
        }));

        // Mapping d·ªØ li·ªáu NVUP
        const nvupData = selectedData.map((item, index) => ({
            "STT": index + 1,
            "MA_LOAI_KCB": 1,
            "MA_KHOA": "",
            "TEN_KHOA": "",
            "MA_BHXH": "",
            "HO_TEN": item.Ho_Ten || "",
            "GIOI_TINH": "",
            "CHUCDANH_NN": "",
            "VI_TRI": "",
            "MACCHN": item.So_CCHN || "",
            "NGAYCAP_CCHN": formatDateToInt(item.Ngay_Cap),
            "NOICAP_CCHN": item.Noi_Cap || "",
            "PHAMVI_CM": "",
            "PHAMVI_CMBS": "",
            "DVKT_KHAC": "",
            "VB_PHANCONG": "",
            "THOIGIAN_DK": 1,
            "THOIGIAN_NGAY": "",
            "THOIGIAN_TUAN": "",
            "CSKCB_KHAC": "",
            "CSKCB_CGKT": "",
            "QD_CGKT": "",
            "TUNGAY": tuNgay
        }));

        createAndDownloadExcel(importData, `IMPORT_PKDK_${maCSKCB}.xlsx`, "DL mau");
        createAndDownloadExcel(nvupData, `NVUP_PKDK_${maCSKCB}.xlsx`, "D·ªØ li·ªáu m·∫´u tham kh·∫£o");
    }

    function createAndDownloadExcel(jsonData, fileName, sheetName) {
        const worksheet = XLSX.utils.json_to_sheet(jsonData);
        const workbook = XLSX.utils.book_new();
        XLSX.utils.book_append_sheet(workbook, worksheet, sheetName);
        XLSX.writeFile(workbook, fileName);
    }

    // --- PH√ÇN TRANG ---
    function changePage(direction) {
        currentPage += direction;
        renderTableBody();
    }
    function updatePaginationControls() {
        const totalPages = Math.ceil(filteredData.length / rowsPerPage);
        document.getElementById('pageIndicator').innerText = `Trang ${currentPage} / ${totalPages || 1}`;
        document.getElementById('prevBtn').disabled = currentPage === 1;
        document.getElementById('nextBtn').disabled = currentPage >= totalPages || totalPages === 0;
    }
</script>

</body>
</html>
